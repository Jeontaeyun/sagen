    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "json": "^10.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.34.2",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": ">=16.8"
  }
